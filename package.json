{
  "name": "@anxing131/tracing",
  "version": "0.0.4",
  "description": "",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhongjixiuxing/tracing.git"
  },
  "license": "MIT",
  "keywords": [
    "koa",
    "jaeger",
    "opentelemetry",
    "tracing"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "watch:build": "tsc -p tsconfig.json -w",
    "reset-hard": "git clean -dfx && git reset --hard && npm i"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@opentelemetry/api": "1.0.4",
    "@opentelemetry/auto-instrumentations-node": "0.27.4",
    "@opentelemetry/exporter-jaeger": "1.0.1",
    "@opentelemetry/instrumentation-http": "0.27.0",
    "@opentelemetry/instrumentation-koa": "0.28.1",
    "@opentelemetry/sdk-node": "0.27.0",
    "@opentelemetry/tracing": "0.24.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-functional": "3.0.2",
    "eslint-plugin-import": "2.22.0",
    "prettier": "2.1.1",
    "ts-node": "9.0.0",
    "typedoc": "0.19.0",
    "typescript": "4.5.5"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "bugs": {
    "url": "https://github.com/zhongjixiuxing/tracing.git"
  },
  "homepage": "https://github.com/zhongjixiuxing/tracing.git",
  "author": "anxing"
}
